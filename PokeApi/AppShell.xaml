<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PokeApi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PokeApi.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="PokeApi">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:jugar}"
        Route="jugar" />
    <ShellContent
        Title="Ranking"
        ContentTemplate="{DataTemplate local:topScores}"
        Route="topScores" />

</Shell>
